{
  "name": "valknut-tree-components",
  "version": "1.0.0",
  "description": "Valknut React Tree Components with Bun Testing and Bundling",
  "type": "module",
  "main": "src/tree-component/index.js",
  "scripts": {
    "test": "bun test",
    "test:e2e": "bun test tests/playwright.e2e.test.ts",
    "test:all": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "build": "bun run build:bundle",
    "build:bundle": "webpack --mode=production",
    "build:dev": "bun build src/tree-component/index.js --outfile=../assets/react-tree-bundle.js --format=iife --global-name=ReactTreeBundle --target=browser --sourcemap",
    "dev": "node scripts/dev-server.cjs",
    "clean": "rm -rf dist node_modules public/data.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ tests/",
    "format": "prettier --write src/ tests/"
  },
  "keywords": [
    "react",
    "tree",
    "code-analysis",
    "valknut",
    "bun"
  ],
  "author": "Valknut",
  "license": "ISC",
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@tanstack/react-virtual": "^3.13.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bun": "latest",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-loader": "^10.0.0",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.6.0",
    "handlebars": "^4.7.8",
    "eslint": "^8.0.0",
    "happy-dom": "^12.0.0",
    "playwright": "^1.55.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.1.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "trustedDependencies": [
    "@esbuild/linux-x64"
  ]
}
